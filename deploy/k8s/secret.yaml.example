# Copy this file to secret.yaml and fill in real values.
# DO NOT commit secret.yaml to the repository.
apiVersion: v1
kind: Secret
metadata:
  name: cad-credentials
  namespace: cad
type: Opaque
stringData:
  CAD_SHOPIFY_SHOP: "<shop-name>"
  CAD_SHOPIFY_API_KEY: "<api-key>"
  CAD_SHOPIFY_PASSWORD: "<api-password>"
  CAD_CF_API_TOKEN: "<cloudflare-api-token>"
  CAD_CF_ZONE_ID: "<cloudflare-zone-id>"
  CAD_MONGO_URI: "mongodb://10.1.1.56:27017"
  CAD_MONGO_DB: "cad"
